<%= simple_form_for card do |f| %>
  <%= f.input :original_text %>
  <%= f.input :translated_text %>
  <%= f.input :image %>
  <%= f.association :deck, label: "Выберите колоду" %>
  <%= f.input :new_deck, label: "Или создайте новую" %>
  <%= f.input :review_date %>
  <%= f.button :submit %>
<% end %>
